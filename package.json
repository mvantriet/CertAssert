{
  "name": "certassert",
  "version": "0.5.2",
  "description": "A ready-to-go solution for an OpenID Connect Provider using X509 Certificate-bound tokens",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "scripts": {
    "build": "CERTASSERT_ENV=prod npm run build:ts && npm run build:types && npm run build:bundle && npm run build:interactions && npm run build:assets",
    "build:interactions": "(cd src/interactions/static && npm run build)",
    "build:bundle": "npm run build:bundle:libs && npm run build:bundle:types",
    "build:bundle:libs": "rollup -c",
    "build:bundle:types": "dts-bundle --name certassert --main build/index.d.ts && mv build/certassert.d.ts dist/index.d.ts",
    "build:ts": "tsc --allowJs",
    "build:assets": "mkdir -p dist/interactions/static/build && cp -rf src/interactions/static/build/* dist/interactions/static/build/",
    "build:types": "tsc --declaration --allowJs --emitDeclarationOnly --project tsconfig.export.json --outDir build",
    "lint": "eslint 'src/**/*.{js,ts,tsx}' --fix",
    "clean": "rm -rf build",
    "start": "npm run start:example",
    "start:e2e": "npm run start:e2eclient & npm run start:example",
    "start:e2eclient": "(cd example/app && npm start)",
    "start:example": "CERTASSERT_ENV=dev npm run start:example:bootstrap && npm run build:interactions && ts-node example/src/app.ts",
    "start:example:bootstrap": "npm run example:certchain && npm run example:servercert && npm run example:signingkeys",
    "example:certchain": "bash ./test/integration/scripts/genCertChain.sh",
    "example:servercert": "bash ./test/integration/scripts/genServerCert.sh",
    "example:signingkeys": "bash ./test/integration/scripts/genSigninKeys.sh",
    "prepare": "npm run build",
    "dev": "nodemon",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mvantriet/certassert.git"
  },
  "keywords": [
    "oidc",
    "OpenIDConnect",
    "certassert",
    "x509",
    "OIDC",
    "rfc8705",
    "certificate-bound",
    "SSO",
    "federated-signin",
    "typescript",
    "authentication"
  ],
  "author": "mvantriet",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mvantriet/CertAssert/issues"
  },
  "homepage": "https://github.com/mvantriet/CertAssert#readme",
  "files": [
    "dist/**/*"
  ],
  "devDependencies": {
    "@babel/cli": "^7.12.16",
    "@babel/core": "^7.12.16",
    "@babel/node": "^7.12.16",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/plugin-transform-runtime": "^7.12.15",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.12.16",
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-url": "^5.0.1",
    "@svgr/rollup": "^5.4.0",
    "@types/asn1js": "^2.0.0",
    "@types/compression": "^1.7.0",
    "@types/express": "^4.17.9",
    "@types/jest": "^26.0.18",
    "@types/node": "^14.14.10",
    "@types/pkijs": "0.0.6",
    "@types/pvutils": "^1.0.0",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/request": "^2.48.4",
    "@types/styled-components": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0",
    "chai": "^4.2.0",
    "eslint": "^7.11.0",
    "jest": "26.6.0",
    "mocha": "^5.2.0",
    "nodemon": "^2.0.7",
    "rollup": "^2.32.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^3.1.8",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.28.0",
    "ts-jest": "^26.4.4",
    "ts-loader": "^7.0.1",
    "ts-node": "^8.9.1",
    "typescript": "^3.9.7",
    "dts-bundle": "^0.7.3"
  },
  "dependencies": {
    "@fidm/x509": "^1.2.1",
    "asn1js": "^2.0.26",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "helmet": "^4.4.1",
    "jose": "^2.0.4",
    "lru-cache": "^6.0.0",
    "multer": "^1.4.2",
    "oidc-provider": "git://github.com/panva/node-oidc-provider.git#v6.31.0",
    "pkijs": "^2.1.91",
    "pvutils": "^1.0.17",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "request": "^2.88.2",
    "styled-components": "^5.2.1",
    "utf8": "^3.0.0"
  }
}
